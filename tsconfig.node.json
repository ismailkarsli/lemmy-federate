{
	"compilerOptions": {
		"tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
		"noEmit": true,
		"target": "esnext",
		"module": "nodenext",
		"rewriteRelativeImportExtensions": true,
		"erasableSyntaxOnly": true,
		"verbatimModuleSyntax": true,
		"skipLibCheck": true,
		"strictNullChecks": true,
		"strict": true,
		"noUncheckedIndexedAccess": false
	},
	"include": ["src", "scripts"]
}
